{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/health", "destination": "/api/index.js" },
    { "source": "/api-docs", "destination": "/api/index.js" },
    { "source": "/api-docs/(.*)", "destination": "/api/index.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}